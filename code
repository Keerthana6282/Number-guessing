import random
n=random.randint(1,100)
guess=int(input("Enter the number : "))
while n!= guess:
    if guess < n:
        print("Too low")
        guess = int(input("Enter number again: "))
    elif guess > n:
        print("Too high!")
        guess = int(input("Enter number again: "))
    else:
      break
print(" Congratulations!!!!!you guessed it right!!")
